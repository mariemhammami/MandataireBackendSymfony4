{% extends 'base.html.twig' %}

{% block title %}Annonces{% endblock %}

{% block body %}
    <h1>Annonces</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ annonce.id }}</td>
            </tr>
            <tr>
                <th>AnnonceId</th>
                <td>{{ annonce.annonceId }}</td>
            </tr>
            <tr>
                <th>DateJugement</th>
